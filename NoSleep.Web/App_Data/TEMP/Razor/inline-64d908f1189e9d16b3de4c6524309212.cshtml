@inherits umbraco.MacroEngines.DynamicNodeContext
                @{
                    var root = Model.AncestorOrSelf();
                    var clients = root.Descendants("Client").Where("Visible");
                    foreach (var client in clients) {
                        <li>
                            <a href="#">@client.Name</a>
                        </li>
                    }
                }